<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="za.co.owfm.UI.SyncWorkOrderValuesFragment">


    <androidx.appcompat.widget.Toolbar
        android:id="@+id/syncWorkOrderValues_toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?attr/colorPrimary"
        android:backgroundTint="@color/white"
        android:elevation="10dp"
        app:titleTextAppearance="@style/Toolbar.TitleText"
        android:minHeight="?attr/actionBarSize"
        android:textAlignment="center"
        android:theme="?attr/actionBarTheme"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:title="Sync Work Order Values" />

    <ImageView
        android:id="@+id/metaWeave_imageView"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:src="@drawable/metaweave_logo"
        app:layout_constraintTop_toBottomOf="@id/syncWorkOrderValues_toolbar"
        app:layout_constraintEnd_toEndOf="@id/syncWorkOrderValues_toolbar"
        app:layout_constraintStart_toStartOf="@id/syncWorkOrderValues_toolbar"
        android:layout_marginTop="12dp"/>



    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/homeFromWOValues_floatingActionButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="24dp"
        app:tint="@color/white"
        android:clickable="true"
        android:src="@drawable/ic_home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>